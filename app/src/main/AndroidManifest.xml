<?xml version="1.0" encoding="utf-8"?>
<manifest package="ru.mts.lessons"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ru.mts.lessons.choose.ChooseActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ru.mts.lessons.activity.SingleActivity" />
        <activity android:name="ru.mts.lessons.mvp.UsersActivity" />
        <activity android:name="ru.mts.lessons.mvvm.MvvmActivity" />

    </application>

</manifest>